#################################################################
#    Classes
#################################################################

###  http://example.org/mcbo#MammalianCellBioprocess
mcbo:MammalianCellBioprocess rdf:type owl:Class ;
                             rdfs:subClassOf iof:ProductProductionProcess ;
                             rdfs:comment "Top application class for mammalian cell-based manufacturing activities." ;
                             rdfs:label "Mammalian cell bioprocess" .

###  http://example.org/mcbo#CellCultureProcess
mcbo:CellCultureProcess rdf:type owl:Class ;
                        rdfs:subClassOf mcbo:MammalianCellBioprocess ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mcbo:hasProcessOutput ;
                                          owl:someValuesFrom mcbo:BioprocessSample
                                        ] ;
                        rdfs:label "Cell culture process" .

###  http://example.org/mcbo#BatchCultureProcess
mcbo:BatchCultureProcess rdf:type owl:Class ;
                         rdfs:subClassOf mcbo:CellCultureProcess ;
                         rdfs:label "Batch culture process" .

###  http://example.org/mcbo#FedBatchCultureProcess
mcbo:FedBatchCultureProcess rdf:type owl:Class ;
                            rdfs:subClassOf mcbo:BatchCultureProcess ;
                            rdfs:label "Fed-batch culture process" .

###  http://example.org/mcbo#PerfusionCultureProcess
mcbo:PerfusionCultureProcess rdf:type owl:Class ;
                             rdfs:subClassOf mcbo:CellCultureProcess ;
                             rdfs:label "Perfusion culture process" .

###  http://example.org/mcbo#ChemostatCultureProcess
mcbo:ChemostatCultureProcess rdf:type owl:Class ;
                             rdfs:subClassOf mcbo:PerfusionCultureProcess ;
                             rdfs:label "Chemostat culture process" .

###  http://example.org/mcbo#ChemostatCultureProcess
mcbo:UnknownCultureProcess rdf:type owl:Class ;
                             rdfs:subClassOf mcbo:CellCultureProcess ;
                             rdfs:label "Unknown culture process: not identifiable from source data" .

###  http://example.org/mcbo#BioprocessSample
mcbo:BioprocessSample rdf:type owl:Class ;
                      rdfs:subClassOf obo:BFO_0000040 ;
                      rdfs:comment "Material aliquot/timepoint produced by a bioprocess for analysis (e.g., RNA-seq)." ;
                      rdfs:label "Bioprocess sample" .

