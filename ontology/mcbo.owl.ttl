###  http://example.org/mcbo#hasGeneExpression
mcbo:hasGeneExpression rdf:type owl:ObjectProperty ;
                       rdfs:domain mcbo:BioprocessSample ;
                       rdfs:range mcbo:GeneExpressionMeasurement ;
                       rdfs:label "has gene expression" .

###  http://purl.obolibrary.org/obo/IAO_0000136
obo:IAO_0000136 rdfs:domain mcbo:GeneExpressionMeasurement ;
                rdfs:range mcbo:Gene ;
                rdfs:label "is about" .

###  http://purl.obolibrary.org/obo/OBI_0000293
obo:OBI_0000293 rdfs:domain obo:OBI_0001271 ;
                rdfs:range mcbo:BioprocessSample ;
                rdfs:label "has specified input" .

###  http://purl.obolibrary.org/obo/OBI_0000299
obo:OBI_0000299 rdfs:domain obo:OBI_0001271 ;
                rdfs:range obo:IAO_0000030 ;
                rdfs:label "has specified output" .

###  https://spec.industrialontologies.org/ontology/core/Core/hasOutput
iof:hasOutput rdf:type owl:ObjectProperty ;
              rdfs:label "has output" .

###  http://example.org/mcbo#hasUnit
mcbo:hasUnit rdf:type owl:ObjectProperty ;
             rdfs:domain mcbo:QuantityValue ;
             rdfs:comment "Object property to a UO unit class (e.g., uo:UO_0000027 for degree Celsius)." ;
             rdfs:label "has unit (UO)" .

