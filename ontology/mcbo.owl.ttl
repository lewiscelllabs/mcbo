###  http://example.org/mcbo#hasQualityMeasurement
mcbo:hasQualityMeasurement rdf:type owl:ObjectProperty ;
                           rdfs:domain mcbo:ProductMaterial ;
                           rdfs:range mcbo:QualityMeasurement ;
                           rdfs:label "has quality measurement" .


mcbo:hasProductivityCategory rdf:type owl:DatatypeProperty ;
    rdfs:domain mcbo:ProductivityMeasurement ;
    rdfs:range xsd:string ;
    rdfs:label "has productivity category" .

mcbo:VeryHighProductivity rdf:type owl:Class ;
    rdfs:subClassOf mcbo:ProductivityMeasurement ;
    rdfs:label "Very High productivity" .

mcbo:HighProductivity rdf:type owl:Class ;
    rdfs:subClassOf mcbo:ProductivityMeasurement ;
    rdfs:label "High productivity" .

mcbo:MediumProductivity rdf:type owl:Class ;
    rdfs:subClassOf mcbo:ProductivityMeasurement ;
    rdfs:label "Medium productivity" .

mcbo:LowMediumProductivity rdf:type owl:Class ;
    rdfs:subClassOf mcbo:ProductivityMeasurement ;
    rdfs:label "Low to Medium productivity" .

mcbo:LowProductivity rdf:type owl:Class ;
    rdfs:subClassOf mcbo:ProductivityMeasurement ;
    rdfs:label "Low productivity" .

###  http://example.org/mcbo#hasProductivityMeasurement
mcbo:hasProductivityMeasurement rdf:type owl:ObjectProperty ;
                                rdfs:domain mcbo:MammalianCellBioprocess ;
                                rdfs:range mcbo:ProductivityMeasurement ;
                                rdfs:label "has productivity measurement" .

###  http://example.org/mcbo#hasCellViabilityMeasurement
mcbo:hasCellViabilityMeasurement rdf:type owl:ObjectProperty ;
                                 rdfs:domain mcbo:BioprocessSample ;
                                 rdfs:range mcbo:CellViabilityMeasurement ;
                                 rdfs:label "has cell viability measurement" .

###  http://example.org/mcbo#analyzedBySequencing
mcbo:analyzedBySequencing rdf:type owl:ObjectProperty ;
