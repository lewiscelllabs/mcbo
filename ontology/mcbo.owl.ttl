@prefix mcbo: <http://example.org/mcbo#> .
@prefix iof: <https://spec.industrialontologies.org/ontology/core/Core/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix uo: <http://purl.obolibrary.org/obo/> .
@base <http://example.org/mcbo#> .

<http://example.org/mcbo> rdf:type owl:Ontology ;
                           owl:imports <http://purl.obolibrary.org/obo/uo.owl> ;
                           rdfs:comment "Comprehensive ontology for mammalian cell bioprocessing: anchors to IOF/BFO and reuses OBO ontologies; designed for RNA-seq analysis, culture condition optimization, and product development." ;
                           rdfs:label "MCBO â€“ Mammalian Cell Bioprocessing Ontology" ;
                           owl:versionInfo "0.2.0-comprehensive" .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/mcbo#hasProcessOutput
mcbo:hasProcessOutput rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf iof:hasOutput ;
                      rdfs:domain mcbo:MammalianCellBioprocess ;
                      rdfs:range mcbo:BioprocessSample ;
                      rdfs:label "has process output (sample)" .

###  http://example.org/mcbo#hasProduct
mcbo:hasProduct rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf iof:hasOutput ;
                rdfs:domain mcbo:MammalianCellBioprocess ;
                rdfs:range mcbo:ProductMaterial ;
                rdfs:comment "Explicit link to the intended product of the bioprocess (e.g., mAb)." ;
                rdfs:label "has product" .

###  http://example.org/mcbo#sampleContainsProduct
mcbo:sampleContainsProduct rdf:type owl:ObjectProperty ;
                           rdfs:domain mcbo:BioprocessSample ;
                           rdfs:range mcbo:ProductMaterial ;
                           rdfs:label "sample contains product" .

###  http://example.org/mcbo#usesCellLine
mcbo:usesCellLine rdf:type owl:ObjectProperty ;
                  rdfs:domain mcbo:MammalianCellBioprocess ;
                  rdfs:range mcbo:CellLine ;
                  rdfs:label "uses cell line" .

###  http://example.org/mcbo#hasClone
mcbo:hasClone rdf:type owl:ObjectProperty ;
              rdfs:domain mcbo:CellLine ;
              rdfs:range mcbo:Clone ;
              rdfs:label "has clone" .

###  http://example.org/mcbo#expressesGene
mcbo:expressesGene rdf:type owl:ObjectProperty ;
                   rdfs:domain mcbo:CellLine ;
                   rdfs:range mcbo:Gene ;
                   rdfs:label "expresses gene" .

###  http://example.org/mcbo#overexpressesGene
mcbo:overexpressesGene rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf mcbo:expressesGene ;
                       rdfs:domain mcbo:CellLine ;
                       rdfs:range mcbo:Gene ;
                       rdfs:label "overexpresses gene" .

###  http://example.org/mcbo#hasCultureConditions
mcbo:hasCultureConditions rdf:type owl:ObjectProperty ;
                          rdfs:domain mcbo:MammalianCellBioprocess ;
                          rdfs:range mcbo:CultureConditions ;
                          rdfs:label "has culture conditions" .

###  http://example.org/mcbo#usesMedium
mcbo:usesMedium rdf:type owl:ObjectProperty ;
                rdfs:domain mcbo:MammalianCellBioprocess ;
                rdfs:range mcbo:CultureMedium ;
                rdfs:label "uses medium" .

###  http://example.org/mcbo#hasNutrientConcentration
mcbo:hasNutrientConcentration rdf:type owl:ObjectProperty ;
                              rdfs:domain mcbo:CultureMedium ;
                              rdfs:range mcbo:NutrientConcentration ;
                              rdfs:label "has nutrient concentration" .


###  http://example.org/mcbo#inCulturePhase
mcbo:inCulturePhase rdf:type owl:ObjectProperty ;
                    rdfs:domain mcbo:BioprocessSample ;
                    rdfs:range mcbo:CulturePhase ;
                    rdfs:label "in culture phase" .

###  http://example.org/mcbo#fromStudy
mcbo:fromStudy rdf:type owl:ObjectProperty ;
               rdfs:domain mcbo:BioprocessSample ;
               rdfs:range mcbo:Study ;
